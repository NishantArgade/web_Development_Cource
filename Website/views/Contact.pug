extends Base.pug

block scripts
      script(src='/static/index.js')
block style
      style
        include ../static/style.css
        include ../static/styleContact.css
block content
       h1 Contact Us
       form(action="/contact" method="post",class="myform")
           input(type="text",name="Name",class="myinput",placeholder="Enter your Name")
           input(type="phone",name="Phone",class="myinput",placeholder="Enter your Phone")
           input(type="email",name="Email",class="myinput",placeholder="Enter your Email")
           input(type="gender",name="Gender",class="myinput",placeholder="Enter your Gender")
           input(type="location",name="Location",class="myinput",placeholder="Enter your Location")
           button(class="btn")
             |Submit